{
  "env": {
      "browser": true,
      "node": true,
      "es2020": true
  },
  "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
  ],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 11,
      "sourceType": "module"
  },
  "plugins": [
      "react",
      "prettier"
  ],
  "rules": {
    "react/prop-types": 0,
    "prettier/prettier": ["error", {
      "endOfLine":"auto"
    }]
  }
}
